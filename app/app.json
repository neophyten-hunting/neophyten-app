{
  "expo": {
    "name": "Neophyten.ch",
    "slug": "Neophyten-ch",
    "owner": "stadt-buelach",
    "version": "0.1.0",
    "orientation": "portrait",
    "primaryColor": "#ffffff",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/*.png"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "ch.neophyten.app",
      "buildNumber": "0.1.0",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Diese App benötigt die Standortdaten deines Gerätes um Neophyten zu erfassen."
      }
    },
    "android": {
      "package": "ch.neophyten.app",
      "versionCode": 1,
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": ""
        }
      }
    },
    "web": {
      "favicon": "./assets/icon.png"
    }
  }
}